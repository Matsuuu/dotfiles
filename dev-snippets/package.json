{
    "name": "Custom Dev Snippets",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": "java",
                "path": "./snippets/java.json"
            },
            {
                "language": [
                    "javascript",
                    "typescript"
                ],
                "path": "./snippets/javascript.json"
            },
            {
                "language": "typescript",
                "path": "./snippets/typescript.json"
            }
        ]
    }
}
